{
  "name": "@ag-cookunity/server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env.production node dist/server.js",
    "prebuild": "npm run prisma:migrate:prod && prisma generate",
    "build": "tsc -p .",
    "dev": "npm run docker:dev:up && npm run prisma:migrate:dev && prisma generate && dotenv -e .env.development nodemon src/server.ts",
    "test": "npm run docker:test:up && npm run prisma:migrate:test && dotenv -e .env.test jest -i --runInBand --detectOpenHandles --forceExit && npm run docker:test:down",
    "test:coverage": "jest --coverage",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate:prod": "dotenv -e .env.production prisma migrate deploy",
    "prisma:migrate:dev": "dotenv -e .env.development prisma migrate deploy",
    "prisma:migrate:test": "dotenv -e .env.test prisma migrate deploy",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d --remove-orphans && sleep 3",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down --remove-orphans",
    "docker:test:up": "docker compose -f docker-compose.test.yml up -d --remove-orphans && sleep 3",
    "docker:test:down": "docker compose -f docker-compose.test.yml down --remove-orphans"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "@prisma/client": "~5.14.0",
    "swagger-ui-express": "~5.0.1",
    "swagger-jsdoc": "~6.2.8",
    "morgan": "~1.10.0",
    "winston": "~3.13.0",
    "openapi-types": ">=7",
    "@ag-cookunity/types": "workspace:*",
    "dotenv-cli": "~7.4.2",
    "yamljs": "~0.3.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "~4.17.21",
    "@types/node": "~20.12.13",
    "nodemon": "^2.0.16",
    "ts-node": "~10.9.2",
    "typescript": "^4.7.4",
    "prisma": "~5.14.0",
    "supertest": "~7.0.0",
    "jest": "~29.7.0",
    "ts-jest": "~29.1.4",
    "@types/jest": "~29.5.12",
    "@types/supertest": "~6.0.2",
    "@types/morgan": "~1.9.9",
    "@types/swagger-ui-express": "~4.1.6",
    "@types/yamljs": "~0.2.34"
  }
}
